                        $("#kaId").val(resJson.kaId);
                        $("#kaboxId").val(resJson.kaboxId);
                        $("#paramter").val(resJson.paramter);
                    }
                }
            });
            event.preventDefault();
        });

        //中转提交
        $("#submitLink").click(function (event) {
            if (validate()) {
                var parameter = $("#paramter").val();
                var actionUrl = $("#actionUrl").val();
                var param_args = "";
                var paramArgsValue = ""; //单纯的值
                var valid = true;
                var blockCount = 0; //有多少个输入框
                $("input[name='param_args']").each(function () {
                    if ($(this).val() != "") {
                        param_args += "&param_args=" + $(this).val();
                        paramArgsValue += $(this).val();
                        blockCount++;
                    } else {
                        valid = false;
                    }

                });
                if (!valid) {
                    return false;
                }
                //去掉多少个\t符号
                var maxLength = 50 - (blockCount - 1) * 2;
                if (paramArgsValue.length > maxLength) {
                    $("#requestErrMsg").remove();
                    $(this).parent().parent().append("<div id='requestErrMsg' style='color:red'>提交的信息长度不能大于" + maxLength + "，请修改后再提交。</div>");
                    return false;
                }
                $.ajax({
                    url: actionUrl,
                    type: 'POST',
                    data: "&requestType=ajax&id=1408097488836692895&kaId=" + $("#kaId").val() + "&paramter=" + parameter + param_args,
                    success: function (result) {
                        var resJson = $.parseJSON($.trim(result));
                        if (resJson.status == "failure"
					  && resJson.loginUrl != null) {
                            window.location = resJson.loginUrl;
                        } else {
                            $("div.code-box").empty().html(resJson.message).show();
                            $("div.playNum").hide();
                            $("#requestPopBox").hide();
                            getSuccess = true;
                            //刷新
                            $("span.close").click();
                        }
                    }
                });
            }
            event.preventDefault();
        });

