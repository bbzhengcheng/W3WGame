        //中转
        $("#confirmSub0,#requestLink").click(function (event) {
            $("#confirmPopBox").hide();
            $.ajax({
                url: "/personal/request.html?requestType=ajax&id=1408097488836692895&currentPageUrl=" + currentPageUrl,
                type: 'GET',
                success: function (result) {
                    var resJson = $.parseJSON(result);
                    if (resJson.status == "failure"
						  && resJson.loginUrl != null) {
                        window.location = resJson.loginUrl;
                    } else if (resJson.status == "failure") {
                        $("#hint_msg").empty().html(resJson.message).show();
                        $("#messageBox").show();
                    } else if (resJson.status == "credentialGot") {
                        var formHtml = "";
                        var transitParam = resJson.transitParam.split(',');
                        var transitContent = resJson.transitContent.split('\t');
                        for (var i = 0; i < transitParam.length; i++) {
                            formHtml += "<p><span class='label'>" + transitParam[i] + "：</span>";
                            formHtml += "<input type='text' name='param_args' class='input' readonly='readonly' value=" + transitContent[i] + " /></p>";
                        }
                        $("#requestCredentialForm").html(formHtml);
                        $("#requestPopCredentialBox").show();
                        $("#requestCredentialErrMsg").remove();
                        //设置隐藏参数
                        $("#actionUrl").val(resJson.actionUrl);
                        $("#kaId").val(resJson.kaId);
                        $("#kaboxId").val(resJson.kaboxId);
                        $("#paramter").val(resJson.paramter);
                    } else if (resJson.status == "mobileNoBind") {
                        var from = resJson.pageForm;
                        messageBindMobile(true, from);
                    } else {
                        var formHtml = "";
                        for (var i = 0; i < resJson.elements.length; i++) {
                            formHtml += "<p><span class='label'>" + resJson.elements[i] + ":</span>";
                            formHtml += "<input type='text' name='param_args' class='input'/></p>";
