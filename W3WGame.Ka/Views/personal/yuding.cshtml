                            msg += "<p>2.<a href='" + qqClockUrl + "' class='link' target='_blank'>接收QQ消息提醒</a>";
                            $("#bookPopTitle").html(msg);
                            $("#bookPopBox").show();
                        } else if (resJson.gameDownloadUrl != null) {  //是否开放下载
                            var msg = "<p>• 您已获得提前30分钟领号的特权，抢号快人一步！请留意站内消息提醒</p>";

                            var gameId = document.getElementById('gameId').value;
                            if (gameId != null && gameId != "" && gameId > 0) {
                                msg += "<p>• 领到的号仅适用于九游游戏客户端，请先下载游戏</p>";

                                var client9 = "<a href='###' onclick='javascript:download9game();'>"
								+ "<span class='l-org'></span><span class='c-org'>下载游戏</span><span class='r-org'></span></a>";
                                $("#9clientDowload").html(client9);
                            }
                            $("#clientbookPopTitle").html(msg);
                            $("#clientbookPopBox").show();
                        } else { //其他场景
                            var msg = "<p>• 您已获得提前30分钟领号的特权，抢号快人一步！</p>";
                            msg += "<p>• 使用九游游戏中心，礼包上架早知道，更有专属福利等着你</p>";
                            var client9 = "<a href='/redirect?a3=pc_book_hint&type=9clientDown&redirectURL=http://app.9game.cn/?ch=KD_59' target='_blank'>"
							+ "<span class='l-org'></span><span class='c-org'>下载九游游戏中心</span><span class='r-org'></span></a>";
                            $("#clientbookPopTitle").html(msg);
                            $("#9clientDowload").html(client9);
                            $("#clientbookPopBox").show();
                        }
                    } else {
                        var msg = "• 恭喜您获得提前30分钟领号的特权！该号";
                        var client9 = "<a href='/redirect?a3=pc_book_hint&type=9clientDown&redirectURL=http://app.9game.cn/?ch=KD_59' target='_blank'>"
						+ "<span class='l-org'></span><span class='c-org'>下载九游游戏中心</span><span class='r-org'></span></a>";
                        msg += "<span><em class='org'>需要使用九游游戏中心进行领取</em>，</span>";
                        msg += "提前下载和安装，抢号可以快人一步哦！";
                        bookSuccess = true;
                        $("#clientbookPopTitle").html(msg);
                        $("#9clientDowload").html(client9);
                        $("#clientbookPopBox").show();
                    }
                }
            });
            event.preventDefault();
        });

