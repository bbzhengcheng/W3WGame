                        var from = resJson.pageForm;
                        messageBindMobile(true, from);
                    } else if (resJson.status == "failure"
						  && resJson.loginUrl != null) {
                        window.location = resJson.loginUrl;
                    } else if (resJson.status == "needValidCode") {	//需要跑验证码
