@{
    Layout = "~/Views/Shared/_WebLayout.cshtml";
}
@using W3WGame.Services.ToolCode
@model WangFramework.MvcPager.PagedList<W3WGame.Core.Dtos.Web.AcceptKaDetailDto>
<div class="main-mod">
    <!--页面模块·面包屑-->
    <div class="crumbs">
        <a href="http://ka.w3wgame.com/">w3w游戏发号中心</a> &gt; <a href="http://ka.w3wgame.com/inbox/kabox.html">
            存号箱</a> &gt; 我预订的号
    </div>
    <!-- main begin -->
    <div class="con">
        <!-- page-con begin -->
        <div class="sub-con pdNone cf grayBG">
            <!-- right-column -->
            <div class="sidebar-main">
                <!-- wrap -->
                <div class="main-wrap">
                    <!-- title -->
                    <div class="user-title">
                        <span class="fl-l">我预订的号</span><!-- <a href="" class="clear-case"><em class="clear-ico"></em>清空存号箱</a> --></div>
                    <!-- gift-list -->
                    <ul class="gift-list cf">
                   @foreach(var item in Model)
                   {
                            <li id="li1411091527763419483">
                            <div class="radius120">
                                <a href="/personal/1410768161395849959.html">
                                    <img src="http://image.game.uc.cn/2014/8/13/9853179.png" alt="">
                                    <span class="radius-mask"></span></a>
                            </div>
                            <div class="g-center cf">
                                <strong class="g-title"><a href="/personal/1410768161395849959.html">[@WebHelper.GetImg(item.ImgPath)]下次测试登录激活码预约</a>
                                </strong>
                                <div class="pro-yu">
                                    <span class="yes-yue">已预订</span> <a href="/inbox/bookingbox/cancel.html?id=1410768161395849959&amp;kaId=1410764682836799748"
                                        style="color: #f60" target="_self">取消预订</a>
                                </div>
                                <p>
                                    使用有效期：@item.StartDate.ToString("yyyy-MM-dd hh:mm:ss") 至 @item.EndDate.ToString("yyyy-MM-dd hh:mm:ss")</p>
                              @*  <div class="g-tag">
                                    <a href="/personal/1410768161395849959.html" class="frist gift-way">礼包内容</a> <a href="/personal/1410768161395849959.html"
                                        class="gift-way">使用方式</a>
                                </div>*@
                            </div>
                            <div class="t-code">
                            </div>
                        </li>

                       }
                    </ul>
                </div>
            </div>
            <!-- left-column -->
            <div class="sidebar-gray">
                <div class="center-status">
                    <!-- 三种状态 -->
                    <a href="/inbox/kabox" target="_self" class="gray-status">我领到的号</a> <a href="/inbox/bookingbox"
                        target="_self" class="btn-status">我预订的号</a> <a href="/inbox/myfollow" target="_self"
                            class="gray-status">我的礼包提醒</a>
                </div>
            </div>
        </div>
        <!-- page-con end -->
    </div>
    <!-- main end -->
    <script type="text/javascript" src="/public/pc/inbox/bookingbox.js"></script>
</div>
