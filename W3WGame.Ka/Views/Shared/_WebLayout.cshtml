    <script type="text/javascript">
        $(function () {
            $("#name").autocomplete({
                source: "/matching.html",
                delay: 10,
                focus: function (event, ui) {
                    var value = ui.item.label.replace(new RegExp("<span class=\"high-light-f60\">", "g"), "");
                    value = value.replace(new RegExp("</span>", "g"), "");
                    $('#name').val(value);
                    return false;
                },
                select: function (event, ui) {
                    var value = ui.item.label.replace(new RegExp("<span class=\"high-light-f60\">", "g"), "");
                    value = value.replace(new RegExp("</span>", "g"), "");
                    $('#name').val(value);
                    return false;
                },
                minLength: 1
            }).data("ui-autocomplete")._renderItem = function (ul, item) {
                return $("<li></li>").data("item.autocomplete", item)
		             .append("<a>" + item.label + "</a>")
		             .appendTo(ul);
            };
            //实时播报滚动
            inRealTime(".new-warp", ".new");
        });
    </script>
